<div class="container">
  <div class="row">
    <%= button_to "Log Out", destroy_trainer_session_path, method: :delete, :class => 'btn sharp logout pull-right', role: 'presentation'%>
    <ol class="breadcrumb col-sm-10">
      <li class="homeBtn"><a href="/game/index">Home</a></li>
      <li><a href="/pokemon/index" class="active">Pokemon</a></li>
      <li>Brewery</li>
      <li class="">Ready Up</li>
      <li class="">Battle</li>
    </ol>
  </div>
  <div class="row">
    <div class="progress">Readiness
      <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%">
        <span class="sr-only">60% Complete (success)</span>
      </div>
    </div>
  </div>
</div>

<div class="container main-body">
  <div class="row search-bars">
    <input type="text" class="form-control sharp search-bar col-sm-3" autocomplete="off" id="brewerySearch" placeholder="Enter a Brewery..."></input>
    <br />
    <button type="submit" class="btn sharp btn-default brewery-search-btn pull-left col-md-1">Search</button>
  </div>
  <br />
  <div class="beerLoadout">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title breweryName"></h3>
      </div>
      <div class="panel-body">
        <ul class="beerNames">
        </ul>
      </div>
    </div>

  </div>
  <a href="/game/loadout"><button type="submit" class="btn sharp pull-left btn-default brew-continue-btn col-md-1">Continue</button></a>
</div>
