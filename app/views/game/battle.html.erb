<div class="container">
  <div class="row">
    <%= button_to "Log Out", destroy_trainer_session_path, method: :delete, :class => 'btn sharp logout pull-right', role: 'presentation'%>
    <ol class="breadcrumb sharp col-sm-10">
      <li class="homeBtn"><a href="/game/index">Home</a></li>
      <li>Pokemon</li>
      <li>Brewery</li>
      <li>Ready Up</a></li>
      <li class="active">Battle</li>
    </ol>
  </div>

  <div class="row">
    <div class="progress sharp">
      <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        <span class="sr-only">100% Complete</span>
      </div>
    </div>

  </div>
</div>
<ul class="pull-left damage-slider">Damage Done:
</ul>
<ul class="pull-left enemyHealth">Enemy Health:
</ul>
<div class="container battle-board-container">
  <h1 class="text-center pulsate"> Battle! </h1>
  <h1 class="countdown text-center"></h1>
  <div class="col-sm-6 pic">
  </div>
</div>

